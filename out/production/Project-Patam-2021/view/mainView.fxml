               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="View">
               <items>
                  <CheckMenuItem fx:id="toggle_graphs" mnemonicParsing="false" selected="true" text="Graphs" />
                  <CheckMenuItem fx:id="toggle_components" mnemonicParsing="false" selected="true" text="Components" />
                  <CheckMenuItem fx:id="toggle_stick" mnemonicParsing="false" selected="true" text="Control Stick" />
               </items>
            </Menu>
            <Menu mnemonicParsing="false" text="Options">
               <items>
                  <Menu mnemonicParsing="false" text="Upload">
                    <items>
                      <MenuItem mnemonicParsing="false" onAction="#openFileExplorer" text="XML Settings" />
                    </items>
                  </Menu>
               </items>
            </Menu>
         </menus>
      </MenuBar>
      <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="500.0" prefWidth="800.0" VBox.vgrow="ALWAYS">
